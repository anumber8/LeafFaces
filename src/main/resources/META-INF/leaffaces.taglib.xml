<?xml version="1.0" encoding="UTF-8"?>
<facelet-taglib xmlns="http://java.sun.com/xml/ns/javaee"
                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
                version="2.0">
    <namespace>http://ofarukkurt.blogspot.com/leaffaces</namespace>
    <tag>
        <tag-name>map</tag-name>
        <component>
            <component-type>com.blogspot.ofarukkurt.leaffaces.component.Map</component-type>
        </component>
        <attribute>
            <name>widgetVar</name>
            <required>true</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>center</name>
            <required>true</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>zoom</name>
            <required>true</required>
            <type>java.lang.Integer</type>
        </attribute>
        <attribute>
            <name>minZoom</name>
            <required>true</required>
            <type>java.lang.Integer</type>
        </attribute>
        <attribute>
            <name>maxZoom</name>
            <required>true</required>
            <type>java.lang.Integer</type>
        </attribute>
        <attribute>
            <name>removeDivHeight</name>
            <type>java.lang.Integer</type>
        </attribute>
        <attribute>
            <name>style</name>
            <type>java.lang.String</type>
        </attribute>
    </tag>
    <tag>
        <tag-name>osmlayer</tag-name>
        <component>
            <component-type>com.blogspot.ofarukkurt.leaffaces.component.OSMLayer</component-type>
        </component>
    </tag>
    <tag>
        <tag-name>locate</tag-name>
        <component>
            <component-type>com.blogspot.ofarukkurt.leaffaces.component.Locate</component-type>
        </component>
        <attribute>
            <name>title</name>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>position</name>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>flyTo</name>
            <type>java.lang.Boolean</type>
        </attribute>
    </tag>
    <tag>
        <tag-name>wms</tag-name>
        <component>
            <component-type>com.blogspot.ofarukkurt.leaffaces.component.Wms</component-type>
        </component>
        <attribute>
            <name>wmsName</name>
            <required>true</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>url</name>
            <required>true</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>layers</name>
            <required>true</required>
            <type>java.lang.String</type>
        </attribute>
    </tag>
    <tag>
        <tag-name>heatmap</tag-name>
        <component>
            <component-type>com.blogspot.ofarukkurt.leaffaces.component.HeatMap</component-type>
        </component>
        <attribute>
            <name>points</name>
            <required>true</required>
            <type>java.util.List</type>
        </attribute>
        <attribute>
            <name>blur</name>
            <type>java.lang.Integer</type>
        </attribute>
        <attribute>
            <name>radius</name>
            <type>java.lang.Integer</type>
        </attribute>
        <attribute>
            <name>maxZoom</name>
            <type>java.lang.Integer</type>
        </attribute>
        <attribute>
            <name>max</name>
            <type>java.lang.Double</type>
        </attribute>
    </tag>
    <tag>
        <tag-name>zoomLevel</tag-name>
        <component>
            <component-type>com.blogspot.ofarukkurt.leaffaces.component.ZoomLevel</component-type>
        </component>
        <attribute>
            <name>title</name>
            <type>java.lang.String</type>
        </attribute>
    </tag>
</facelet-taglib>
